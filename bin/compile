#! /usr/bin/env bash
BUILD_DIR=$1
CACHE_DIR=$2
../python-bin/detect $BUILD_DIR $CACHE_DIR > /dev/null
$pr = $?
../ruby-bin/detect $BUILD_DIR $CACHE_DIR > /dev/null
$rr = $?
if [ $pr -eq 0 ]; then
    ../python-bin/compile $BUILD_DIR $CACHE_DIR
fi
if [ $rr -eq 0 ]; then
    ../ruby-bin/compile $BUILD_DIR $CACHE_DIR
fi
